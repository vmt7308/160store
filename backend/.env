# Tắt thông báo dotenv
DOTENV_CONFIG_QUIET=true

# Thêm keys cho 10 free AI APIs (lấy free từ các trang tương ứng)
GEMINI_API_KEY=your_gemini_free_key  # Google AI Studio, link: https://aistudio.google.com/app/api-keys?referrer=grok.com
HF_API_KEY=your_huggingface_key  # Hugging Face, link: https://huggingface.co/settings/tokens?referrer=grok.com
REPLICATE_API_KEY=your_replicate_free_key  # Replicate.ai free tier, link: https://replicate.com/account/api-tokens?referrer=grok.com
TOGETHER_API_KEY=your_togetherai_free_key  # Together.ai free, link: https://api.together.xyz/
GROQ_API_KEY=your_groq_free_key  # Groq free tier, link: https://console.groq.com/keys?referrer=grok.com
COHERE_API_KEY=your_cohere_free_key  # Cohere free tier, link: https://dashboard.cohere.com/api-keys?referrer=grok.com
PERPLEXITY_API_KEY=your_perplexity_free_key  # Perplexity free (if available), link: https://www.perplexity.ai/account/api/group
MISTRAL_API_KEY=your_mistral_free_key  # Mistral AI free tier, link: https://console.mistral.ai/home?workspace_dialog=apiKeys
LLAMA_API_KEY=your_llama_free_key  # Llama via free endpoint, Không cần key riêng. Code dùng Groq/Together để chạy Llama models miễn phí.
AYA_API_KEY=your_aya_free_key  # Cohere Aya free, Aya là model của Cohere → dùng chung key Cohere. Không cần key riêng.